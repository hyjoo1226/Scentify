package com.ssafy.scentify.combination.model.repository;

import org.apache.ibatis.annotations.Insert;
import org.apache.ibatis.annotations.Mapper;

import com.ssafy.scentify.device.model.dto.DeviceDto.defaultCombinationDto.Combination;

@Mapper
public interface CombinationRepository {
	
	// combination 등록
	@Insert("INSERT INTO device (id, choice_1, choice_1_count, ip_address)"
    		+ "VALUES (#{combinationId}, #{serial}, #{adminId}, #{ipAddress})")
	boolean createCombination(Integer combinationId, Combination combination);
}
